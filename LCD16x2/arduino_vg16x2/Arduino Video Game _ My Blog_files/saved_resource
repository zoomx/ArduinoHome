	position:absolute;
	top:0;
	bottom:0;
	left:0;
	right:0;
	display:block;
}

div#jp-relatedposts div.jp-relatedposts-items p {
	margin-bottom: 0;
}

div#jp-relatedposts div.jp-relatedposts-items-visual h4.jp-relatedposts-post-title {
	text-transform: none;
	margin: 0;
	font-family: inherit;
	display: block;
	max-width: 100%;
}

